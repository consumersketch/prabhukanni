<?php

namespace Acme\Bundle\TestBundle\Entity;

use Doctrine\ORM\EntityRepository;

/**
 * This class was generated by the Doctrine ORM. Add your own custom
 * repository methods below.
 */
class ClientsRepository extends EntityRepository
{
	// repository function to fetch data for all the clients using client entity
	public function getAllClients(){
		
    	return $this->findBy(array(), array('clientId' => 'ASC'));
    }
}
